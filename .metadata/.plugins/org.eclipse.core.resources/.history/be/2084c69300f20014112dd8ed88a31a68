import org.teachingextensions.logo.Tortoise;
public class methods {
	public static void main(String[] args) {
		new methods();
	}
methods(){
	
  square(21);	
  }

void square(int side)
{
	for (int i = 0; i < 4; i++)
	{
		Tortoise.setSpeed(10);
		Tortoise.move(side);
		Tortoise.turn(90);
		
	}
	
}


}
	
	
	
	

